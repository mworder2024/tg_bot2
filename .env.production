# Production Environment Configuration for Railway
NODE_ENV=production
PORT=3000

# Database Configuration (Railway will provide DATABASE_URL)
DB_TYPE=postgres
DB_SYNCHRONIZE=false
DB_LOGGING=false

# Telegram Bot Configuration
# BOT_TOKEN will be set in Railway environment variables
BOT_WEBHOOK_URL=
BOT_WEBHOOK_SECRET=

# Security
# JWT_SECRET will be set in Railway environment variables
JWT_EXPIRES_IN=24h

# Rate Limiting
RATE_LIMIT_TTL=60000
RATE_LIMIT_LIMIT=100

# CORS
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info